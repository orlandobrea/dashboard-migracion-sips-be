{
  "release": {
    "branches": ["master"],
    "repositoryUrl": "https://github.com/orlandobrea/dashboard-migracion-sips-be.git"
  },
  "generateNotes": true,
  "plugins": [
    [
      "@semantic-release/release-notes-generator",
      {
        "preset": "express",
        "parserOpts": {
          "noteKeywords": ["BREAKING CHANGE", "BREAKING CHANGES", "BREAKING"]
        },
        "writerOpts": {
          "commitsSort": ["subject", "scope"]
        }
      }
    ]
  ]
}
